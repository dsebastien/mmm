  "dependencies": {
	"babel-runtime": "6.3.x",
	"angular2": "2.0.0-alpha.46",
	"@reactivex/rxjs": "5.0.0-alpha.7",
	"reflect-metadata": "0.1.2",
	"zone.js": "0.5.8",
	"firebase": "2.3.x"
  },
  "devDependencies": {
	"babel-core": "6.3.x",
	"babel-plugin-transform-es2015-modules-commonjs": "6.3.x",
	"babel-preset-es2015": "6.3.x",
	"gulp": "3.9.x",
	"jspm": "0.16.x",
	"nodemon": "1.8.x",
	"typescript": "1.7.3",
	"modern-web-dev-build": "0.1.1",
	"jasmine": "2.4.x",
	"karma-jasmine": "0.3.x",
